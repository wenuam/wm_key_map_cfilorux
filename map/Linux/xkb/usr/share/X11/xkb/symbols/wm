    // | INS  │ HOME │>PGUP │
    // | :118 | :110 | :112 |
    // |>DELE │ END  │>PGDN │
    // | :119 | :115 | :117 |

    //              NORMAL            , SHIFT             , MODE              , MODE + SHIFT      , ALTGR             , ALTGR + SHIFT     ,
//  key <INS>   { [ Insert            ,                   ,                   ,                   ] }; //
//  key <HOME>  { [ Home              ,                   ,                   ,                   ] }; //
    key <PGUP>  { [ Prior             , Prior             , Home              , Home              ] }; //
    key <DELE>  { [ Delete            , Delete            , Insert            , Insert            ] }; //
//  key <END>   { [ End               ,                   ,                   ,                   ] }; //
    key <PGDN>  { [ Next              , Next              , End               , End               ] }; //

    // |      │  UP  │      │
    // |      | :111 |      |
    // | LEFT │ DOWN │ RGHT │
    // | :113 | :116 | :114 |

    //              NORMAL            , SHIFT             , MODE              , MODE + SHIFT      , ALTGR             , ALTGR + SHIFT     ,
    key <UP>    { [ Up                , Up                , Prior             , Prior             ] }; //
    key <LEFT>  { [ Left              , Left              , Home              , Home              ] }; //
    key <DOWN>  { [ Down              , Down              , Next              , Next              ] }; //
    key <RGHT>  { [ Right             , Right             , End               , End               ] }; //

